package prueba;

public class EjClase {
	private int patas;
	private String color;
	private int ancho;
	private int largo;
	private int alto;
	public int getPatas ( ) {
		return patas;
	}
	
	public void setPatas ( int alto ) {
	this.alto = alto;
	}
	
	 int setEleva (int alto) {
		int eleva;
		eleva = eleva + alto;
		return eleva;
	}
	

}
